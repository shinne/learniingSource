<div id="{settingWrapId}" class="summary-setting-wrap">
    <a id="{settingIconId}" class="summary-setting-icon" href="javascript:;">
        <i class="fa fa-cog "></i>
    </a>
    <div id="{settingPannelId}" class="summary-setting-panel">
        <div class="summary-setting-panel-arrows"></div>
        <div class="summary-setting-panel-body">
            <div class="summary-setting-panel-text">
                <span>{maxNum}</span>
                <a class="summary-reset" href="javascript:;">{resetDefault}</a>
            </div>
            <ul id="{settingUlId}" class="summary-setting-ul"></ul>
        </div>
        <div class="summary-setting-panel-footer">
            <button id="{btnSubmitId}" class="btn btn-primary" type="button">{btnSubmit}</button>
            <button id="{btnCancelId}" class="btn btn-default" type="button">{btnCancel}</button>
        </div>
    </div>
</div>
